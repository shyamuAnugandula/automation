#Using string as input
Palindrome=input("Enter a value or String: ")
if(Palindrome==Palindrome[::-1]):
    print(str(Palindrome)+" is palindrome")
else:
    print("Entered "+str(Palindrome)+" is not a palindrome")


#Using List as input
# Even if you Enter a String or Number
Palindrome=list(input("Enter a value or String: "))
if(Palindrome==Palindrome[::-1]):
    print(str(Palindrome)+" is palindrome")
else:
    print("Entered"+str(Palindrome)+"is not a palindrome")
